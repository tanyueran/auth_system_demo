<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.tanyueran.auth_system_springboot_mapper.mapper.UserMapper">
    <select id="getAllUser" resultType="com.github.tanyueran.auth_system_springboot_pojo.bean.User">
    select * from `user_table`
  </select>

    <select id="getUserById" resultType="com.github.tanyueran.auth_system_springboot_pojo.bean.User">
        SELECT * FROM `user_table` where id = #{id}
    </select>
</mapper>